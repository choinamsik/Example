for i in range(1, 10 , 1):
    print(i, end = ' ')
    print('단', end =' ')
print()
for j in range(1, 10, 1):
    print(i, end = " ")
    print('*', end = " ")
    print(j, end = " ")
    print('=', end = " ")
    print(i*j, end = " ")
    print()
print()


---------------------------------------------------------------------------------------------------------------


sum = 0
for i in range(1, 11, 1):
    sum = sum + i
print(sum)


for i in range(1, 11, 2):
    sum = sum + i
print(sum)

for i in range(0, 11, 2):
    sum = sum + i
print(sum)



# 1부터 100까지 짝수의 합
for i in range(0, 101, 2):
    sum = sum + i
print(sum)
# 1부터 100까지 홀수의 합
for i in range(1, 101, 2):
    sum = sum + i
print(sum)

print('-------------------------------------')
for i in range(1, 10, 1):
    print(i)
print('-------------------------------------')
for i in range(10, 1, -1):
    print(i)
    
    
---------------------------------------------------------------------------------------------------------------


num = 5
for i in range(9, 0, -1):
    print(num, end = '')
    print('*', end = '')
    print(i, end = '')
    print('=', end = '')
    print(num*i, end = '')
    print()
    
    
---------------------------------------------------------------------------------------------------------------


for i in range(2, 10, 1):
    print(i, end = " ")
    print('단', end = " ")
    print()
    for u in range(1, 10, 1):
        print( u, end='')
        print( "*", end='')
        print( i, end='')
        print( "=", end='')
        print( u*i, end='')
        print()
    print()
    
    
    
---------------------------------------------------------------------------------------------------------------

tmp = int(input('원하는 구구단을 입력하세요. >>>> '' '))

for i in range(1, 10, 1):
    print(i, end = " ")
    print("*", end = ' ' )
    print(tmp, end = ' ' )
    print("=", end = ' ' )
    print(i*tmp, end = ' ' )
    print()
print()


